{
    "name": "tbcd/ftp-client",
    "type": "library",
    "description": "Create ftp clients services to be able to manage multiple ftp connection with a common abstraction for all protocols (FTP, SFTP, SCP)",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "keywords": [
        "ftp",
        "sftp",
        "scp"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "ext-ssh2": "*",
        "symfony/options-resolver": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\FtpClient\\": "src/"
        }
    }
}
