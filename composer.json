{
    "name": "tbcd/ftp-client",
    "type": "library",
    "license": "MIT",
    "description": "FTP client library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "phpseclib/phpseclib": "^3.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/options-resolver": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\FtpClient\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
